<h1><strong>Admin Login</strong></h1>
<%= flash[:message]  if flash[:message]%>
<%= form_for :admin do |f| %>

<%= f.label :email %>
<%= f.email_field :email%><br><br>

<%= f.label :password %>
<%= f.password_field :password%><br><br>

<%= f.submit "Submit" %>

<% end %>
<div class="buttons">
<%= link_to "Login In with Google", '/auth/google_oauth2', :class => "button is-dark"%>
</div>